{
    "manifest_version": 3,
    "name": "NoteArchive",
    "description": "Your personal note vault",
    "version": "1.3",
    "action": {
      "default_popup": "popup/popup.html"
    },
    "permissions": [
        "storage"
    ],
    "icons": {
      "16": "images/logo/16.png",
      "32": "images/logo/32.png",
      "48": "images/logo/48.png",
      "64": "images/logo/64.png",
      "128": "images/logo/128.png"
    }
  }